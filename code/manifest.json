{
  "name": "GotoKnow Express",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Easily write your GotoKnow journal entries with this Chrome extension",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://www.gotoknow.org/",
    "http://www.kv.dev/"
  ]
}
