{
  "name": "GotoKnow Express",
  "version": "0.1.4",
  "manifest_version": 2,
  "description": "Easily write your GotoKnow journal entries with this Chrome extension",
  "icons": {
    "16": "images/icon-16.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "browser_action": {
    "default_icon": "images/icon-19.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://www.gotoknow.org/"
  ]
}
